<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean id="employee" class="com.capg.programme1and2.Employee">
        <property name="employeeId" value="1"></property>
        <property name="employeeName" value="harsh"></property>
        <property name="salary" value="50000.00"></property>
        <property name="busiessUnit" ref="sbu">
        
        </property>
        
        
        </bean>
        <bean id="sbu" class="com.capg.programme1and2.Sbu"  >
        <property name="sbuId" value="1"></property>
        <property name="sbuName" value="harsh"></property>
        <property name="sbuHead" value="Kumar"></property>
       
        </bean>
        
        </beans>